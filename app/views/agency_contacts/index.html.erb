<p id="notice"><%= notice %></p>

<h1>联系人</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Agency</th>
      <th>Name</th>
      <th>Mobile</th>
      <th>Is admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agency_contacts.each do |agency_contact| %>
      <tr>
        <td><%= agency_contact.agency %></td>
        <td><%= agency_contact.name %></td>
        <td><%= agency_contact.mobile %></td>
        <td><%= agency_contact.is_admin %></td>
        <td><%= link_to 'Show', agency_contact %></td>
        <td><%= link_to 'Edit', edit_agency_contact_path(agency_contact) %></td>
        <td><%= link_to 'Destroy', agency_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

