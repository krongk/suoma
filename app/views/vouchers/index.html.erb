<p id="notice"><%= notice %></p>

<h1>Listing Vouchers</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Traveller group</th>
      <th>Price</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vouchers.each do |voucher| %>
      <tr>
        <td><%= voucher.traveller_group %></td>
        <td><%= voucher.price %></td>
        <td><%= voucher.image %></td>
        <td><%= link_to 'Show', voucher %></td>
        <td><%= link_to 'Edit', edit_voucher_path(voucher) %></td>
        <td><%= link_to 'Destroy', voucher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voucher', new_voucher_path %>
